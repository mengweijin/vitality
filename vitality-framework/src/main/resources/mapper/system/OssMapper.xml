<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mengweijin.vitality.system.mapper.OssMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.mengweijin.vitality.system.domain.entity.Oss">
        <id column="ID" property="id" />
        <result column="NAME" property="name" />
        <result column="SUFFIX" property="suffix" />
        <result column="STORAGE_PATH" property="storagePath" />
        <result column="MD5" property="md5"/>
        <result column="CREATE_BY" property="createBy" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="UPDATE_BY" property="updateBy" />
        <result column="UPDATE_TIME" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="BaseColumnList">
        NAME,
        SUFFIX,
        STORAGE_PATH,
        MD5
        ID,
        CREATE_BY,
        CREATE_TIME,
        UPDATE_BY,
        UPDATE_TIME
    </sql>

    <!-- 通用查询条件 -->
    <sql id="WhereCondition">
        <where>
            <if test="name != null and name != ''">
                AND NAME = #{name}
            </if>
            <if test="suffix != null and suffix != ''">
                AND SUFFIX = #{suffix}
            </if>
            <if test="storagePath != null and storagePath != ''">
                AND STORAGE_PATH = #{storagePath}
            </if>
            <if test="md5 != null and md5 != ''">
                AND MD5 = #{md5}
            </if>
            <if test="id != null">
                AND ID = #{id}
            </if>
            <if test="createBy != null">
                AND CREATE_BY = #{createBy}
            </if>
            <if test="createTime != null">
                AND CREATE_TIME = #{createTime}
            </if>
            <if test="updateBy != null">
                AND UPDATE_BY = #{updateBy}
            </if>
            <if test="updateTime != null">
                AND UPDATE_TIME = #{updateTime}
            </if>
        </where>
    </sql>

</mapper>
